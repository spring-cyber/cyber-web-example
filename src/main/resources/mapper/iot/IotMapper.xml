<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.oyetalk.iot.domain.repository.IotMapper">	<sql id="baseColumns">		 id, device_type, device_type_name, device_id, device_name, device_cover, device_status, device_status_name, device_position, device_position_name, deleted, status, create_time, creator_id, update_time, updator_id, remark	</sql>	<sql id="updateColumns">		<set>			  <if test="id != null">                id   = #{id},              </if>              <if test="deviceType != null">                device_type   = #{deviceType},              </if>              <if test="deviceTypeName != null">                device_type_name   = #{deviceTypeName},              </if>              <if test="deviceId != null">                device_id   = #{deviceId},              </if>              <if test="deviceName != null">                device_name   = #{deviceName},              </if>              <if test="deviceCover != null">                device_cover   = #{deviceCover},              </if>              <if test="deviceStatus != null">                device_status   = #{deviceStatus},              </if>              <if test="deviceStatusName != null">                device_status_name   = #{deviceStatusName},              </if>              <if test="devicePosition != null">                device_position   = #{devicePosition},              </if>              <if test="devicePositionName != null">                device_position_name   = #{devicePositionName},              </if>              <if test="deleted != null">                deleted   = #{deleted},              </if>              <if test="status != null">                status   = #{status},              </if>              <if test="createTime != null">                create_time   = #{createTime},              </if>              <if test="creatorId != null">                creator_id   = #{creatorId},              </if>              <if test="updateTime != null">                update_time   = #{updateTime},              </if>              <if test="updatorId != null">                updator_id   = #{updatorId},              </if>              <if test="remark != null">                remark   = #{remark},              </if>		</set>	</sql>    <sql id="whereClause">        <trim prefix="WHERE" prefixOverrides="AND">			  <if test="id != null">                    AND ${columnPrefix}id = #{id}              </if>              <if test="deviceType != null">                    AND ${columnPrefix}device_type = #{deviceType}              </if>              <if test="deviceTypeName != null">                    AND ${columnPrefix}device_type_name = #{deviceTypeName}              </if>              <if test="deviceId != null">                    AND ${columnPrefix}device_id = #{deviceId}              </if>              <if test="deviceName != null">                    AND ${columnPrefix}device_name = #{deviceName}              </if>              <if test="deviceCover != null">                    AND ${columnPrefix}device_cover = #{deviceCover}              </if>              <if test="deviceStatus != null">                    AND ${columnPrefix}device_status = #{deviceStatus}              </if>              <if test="deviceStatusName != null">                    AND ${columnPrefix}device_status_name = #{deviceStatusName}              </if>              <if test="devicePosition != null">                    AND ${columnPrefix}device_position = #{devicePosition}              </if>              <if test="devicePositionName != null">                    AND ${columnPrefix}device_position_name = #{devicePositionName}              </if>              <if test="deleted != null">                    AND ${columnPrefix}deleted = #{deleted}              </if>              <if test="status != null">                    AND ${columnPrefix}status = #{status}              </if>              <if test="createTime != null">                    AND ${columnPrefix}create_time = #{createTime}              </if>              <if test="creatorId != null">                    AND ${columnPrefix}creator_id = #{creatorId}              </if>              <if test="updateTime != null">                    AND ${columnPrefix}update_time = #{updateTime}              </if>              <if test="updatorId != null">                    AND ${columnPrefix}updator_id = #{updatorId}              </if>              <if test="remark != null">                    AND ${columnPrefix}remark = #{remark}              </if>        </trim>    </sql>		<insert id="save" parameterType="Iot">		insert into Iot 		( id, device_type, device_type_name, device_id, device_name, device_cover, device_status, device_status_name, device_position, device_position_name, deleted, status, create_time, creator_id, update_time, updator_id, remark)         values        ( #{id},         #{deviceType},         #{deviceTypeName},         #{deviceId},         #{deviceName},         #{deviceCover},         #{deviceStatus},         #{deviceStatusName},         #{devicePosition},         #{devicePositionName},         #{deleted},         #{status},         #{createTime},         #{creatorId},         #{updateTime},         #{updatorId},         #{remark})	</insert>		<delete id="deleteById" parameterType="Iot">		DELETE FROM Iot WHERE id = #{id} and tenant_code = #{tenantCode}	</delete>		<update id="updateById" parameterType="Iot">		update Iot		<include refid="updateColumns" />		where id = #{id}		and tenant_code = #{tenantCode}	</update>		<select id="selectOne" resultType="Iot" parameterType="Iot">		SELECT		<include refid="baseColumns" />		FROM Iot        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>		<select id="selectByIndexCount" parameterType="Iot" resultType="int">		SELECT count(1)		FROM Iot        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>	<select id="selectByIndex" parameterType="Iot" resultType="Iot">		SELECT		<include refid="baseColumns" />		FROM Iot        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>		ORDER BY #{sortBy} #{sortType}		LIMIT #{offset} , #{limit}	</select></mapper>