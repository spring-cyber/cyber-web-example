<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.oyetalk.example1.domain.repository.ApplicationMapper">	<sql id="baseColumns">		 id, tenant_code, application_code, application_name, application_ak, application_sk, deleted, status, create_time, creator_code, update_time, updator_code, remark	</sql>	<sql id="updateColumns">		<set>			  <if test="id != null">                id   = #{id},              </if>              <if test="tenantCode != null">                tenant_code   = #{tenantCode},              </if>              <if test="applicationCode != null">                application_code   = #{applicationCode},              </if>              <if test="applicationName != null">                application_name   = #{applicationName},              </if>              <if test="applicationAk != null">                application_ak   = #{applicationAk},              </if>              <if test="applicationSk != null">                application_sk   = #{applicationSk},              </if>              <if test="deleted != null">                deleted   = #{deleted},              </if>              <if test="status != null">                status   = #{status},              </if>              <if test="createTime != null">                create_time   = #{createTime},              </if>              <if test="creatorCode != null">                creator_code   = #{creatorCode},              </if>              <if test="updateTime != null">                update_time   = #{updateTime},              </if>              <if test="updatorCode != null">                updator_code   = #{updatorCode},              </if>              <if test="remark != null">                remark   = #{remark},              </if>		</set>	</sql>    <sql id="whereClause">        <trim prefix="WHERE" prefixOverrides="AND">			  <if test="id != null">                    AND ${columnPrefix}id = #{id}              </if>              <if test="tenantCode != null">                    AND ${columnPrefix}tenant_code = #{tenantCode}              </if>              <if test="applicationCode != null">                    AND ${columnPrefix}application_code = #{applicationCode}              </if>              <if test="applicationName != null">                    AND ${columnPrefix}application_name = #{applicationName}              </if>              <if test="applicationAk != null">                    AND ${columnPrefix}application_ak = #{applicationAk}              </if>              <if test="applicationSk != null">                    AND ${columnPrefix}application_sk = #{applicationSk}              </if>              <if test="deleted != null">                    AND ${columnPrefix}deleted = #{deleted}              </if>              <if test="status != null">                    AND ${columnPrefix}status = #{status}              </if>              <if test="createTime != null">                    AND ${columnPrefix}create_time = #{createTime}              </if>              <if test="creatorCode != null">                    AND ${columnPrefix}creator_code = #{creatorCode}              </if>              <if test="updateTime != null">                    AND ${columnPrefix}update_time = #{updateTime}              </if>              <if test="updatorCode != null">                    AND ${columnPrefix}updator_code = #{updatorCode}              </if>              <if test="remark != null">                    AND ${columnPrefix}remark = #{remark}              </if>        </trim>    </sql>		<insert id="save" parameterType="Application">		insert into test_application		( id, tenant_code, application_code, application_name, application_ak, application_sk, deleted, status, create_time, creator_code, update_time, updator_code, remark)         values        ( #{id},         #{tenantCode},         #{applicationCode},         #{applicationName},         #{applicationAk},         #{applicationSk},         #{deleted},         #{status},         #{createTime},         #{creatorCode},         #{updateTime},         #{updatorCode},         #{remark})	</insert>		<delete id="deleteById" parameterType="Application">		DELETE FROM test_application WHERE id = #{id} and tenant_code = #{tenantCode}	</delete>		<update id="updateById" parameterType="Application">		update test_application		<include refid="updateColumns" />		where id = #{id}		and tenant_code = #{tenantCode}	</update>		<select id="selectOne" resultType="Application" parameterType="Application">		SELECT		<include refid="baseColumns" />		FROM test_application        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>		<select id="selectByIndexCount" parameterType="Application" resultType="int">		SELECT count(1)		FROM test_application        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>	</select>	<select id="selectByIndex" parameterType="Application" resultType="Application">		SELECT		<include refid="baseColumns" />		FROM test_application        <include refid="whereClause">            <property name="columnPrefix" value=""/>        </include>		ORDER BY #{sortBy} #{sortType}		LIMIT #{offset} , #{limit}	</select></mapper>